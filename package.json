{
    "name": "documentation-maker",
    "displayName": "Documentation Maker",
    "description": "Documentation extension for VS Code",
    "version": "0.1.5",
    "publisher": "akshaychame",
    "engines": {
      "vscode": "^1.60.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
      "onStartupFinished",
      "onCommand:doc-maker.initialSetup",
      "onCommand:doc-maker.updateDocs"
    ],
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "doc-maker.initialSetup",
          "title": "Documentation Maker: Initial Setup",
          "category": "Documentation Maker"
        },
        {
          "command": "doc-maker.updateDocs",
          "title": "Documentation Maker: Update Documentation",
          "category": "Documentation Maker"
        }
      ],
      "keybindings": [
        {
          "command": "doc-maker.initialSetup",
          "key": "ctrl+shift+d ctrl+shift+i",
          "mac": "cmd+shift+d cmd+shift+i",
          "when": "editorFocus"
        },
        {
          "command": "doc-maker.updateDocs",
          "key": "ctrl+shift+d ctrl+shift+u",
          "mac": "cmd+shift+d cmd+shift+u",
          "when": "editorFocus"
        }
      ],
      "configuration": {
        "title": "Documentation Maker",
        "properties": {
          "doc-maker.geminiApiKey": {
            "type": "string",
            "default": "",
            "description": "API Key for Gemini 2.5"
          }
        }
      }
    },
    "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
    },
    "dependencies": {
      "axios": "^1.6.3"
    },
    "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/glob": "^7.1.3",
      "@types/node": "16.x",
      "eslint": "^8.47.0",
      "glob": "^8.1.0"
    }
  }